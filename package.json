{
  "name": "hyperliquid-shielded-twap",
  "version": "1.0.0",
  "description": "Advanced Shielded TWAP Executor leveraging Hyperliquid's unique architecture",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "deploy": "npx hardhat run scripts/deploy.js --network hyperliquid-testnet",
    "demo:create": "npx hardhat run scripts/demo/create-twap.js --network hyperliquid-testnet",
    "demo:monitor": "npx hardhat run scripts/demo/monitor-execution.js --network hyperliquid-testnet",
    "demo:analytics": "npx hardhat run scripts/demo/analytics.js --network hyperliquid-testnet",
    "demo:full": "npm run compile && npm run deploy && npm run demo:create && npm run demo:monitor",
    "verify": "npx hardhat verify --network hyperliquid-testnet",
    "clean": "npx hardhat clean",
    "node": "npx hardhat node",
    "console": "npx hardhat console --network hyperliquid-testnet"
  },
  "keywords": [
    "hyperliquid",
    "twap",
    "defi",
    "mev-protection",
    "precompiles",
    "corewriter"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=18.0.0",
    "chai": "^4.2.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0"
  }
}